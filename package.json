{
  "name": "precipitacion_web_api",
  "version": "1.0.0",
  "description": "API Rest para el Sistema de Precipitación en Web de la Institución Alternativas y Procesos de Participación Social",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "set NODE_ENV=dev && nodemon ./",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "author": "Jair Escamilla",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^14.3.0",
    "express": "^4.17.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.14.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1"
  }
}